  "protocols": {
    "cccam": {
      "default_port": 12000,
      "timeout": 15,
      "max_retries": 3,
      "retry_delay": 2,
      "supported_versions": ["2.0.11", "2.1.3", "2.1.4", "2.2.1", "2.3.0", "2.3.2"],
      "description": "CCcam Card Sharing Protocol"
    },
    "newcamd": {
      "default_port": 15000,
      "timeout": 12,
      "max_retries": 2,
      "retry_delay": 1,
      "supported_versions": ["5.25", "6.0", "6.1"],
      "des_key_required": true,
      "description": "NewCamd Protocol with DES encryption"
    },
    "mgcamd": {
      "default_port": 15000,
      "timeout": 12,
      "max_retries": 2,
      "retry_delay": 1,
      "supported_versions": ["1.35", "1.38", "1.40"],
      "description": "MGcamd Protocol (NewCamd compatible)"
    },
    "oscam": {
      "default_port": 988,
      "web_ports": [8888, 8080, 16001],
      "timeout": 15,
      "max_retries": 3,
      "retry_delay": 2,
      "supported_versions": ["1.20", "1.30", "11708", "11709", "11710"],
      "web_interface_required": true,
      "description": "OSCam Open Source Conditional Access Module"
    }
  },

  "analysis": {
    "max_concurrent_connections": 10,
    "enable_deep_analysis": true,
    "package_detection": true,
    "version_detection": true,
    "expiry_estimation": true,
    "response_time_tracking": true
  },

  "expiry_patterns": {
    "test_accounts": {
      "keywords": ["test", "trial", "demo", "temp", "sample"],
      "default_days": 3,
      "description": "Test/trial accounts with short expiry"
    },
    "monthly_accounts": {
      "keywords": ["month", "30d", "m1", "monthly", "30days"],
      "default_days": 30,
      "description": "Monthly subscription accounts"
    },
    "yearly_accounts": {
      "keywords": ["year", "365", "y1", "12m", "annual", "yearly"],
      "default_days": 365,
      "description": "Yearly subscription accounts"
    },
    "premium_accounts": {
      "keywords": ["vip", "premium", "platinum", "ultimate", "pro"],
      "default_days": 90,
      "description": "Premium accounts with extended access"
    },
    "default": {
      "days": 30,
      "description": "Default expiry for unrecognized patterns"
    }
  },

