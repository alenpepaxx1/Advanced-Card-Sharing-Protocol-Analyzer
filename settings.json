  "gui": {
    "theme": "dark",
    "window_size": {
      "width": 1400,
      "height": 900
    },
    "colors": {
      "background": "#2b2b2b",
      "foreground": "#ffffff",
      "select_background": "#404040",
      "select_foreground": "#ffffff",
      "button_background": "#404040",
      "button_foreground": "#ffffff",
      "entry_background": "#404040",
      "entry_foreground": "#ffffff",
      "text_background": "#1e1e1e",
      "text_foreground": "#ffffff",
      "success": "#4CAF50",
      "error": "#f44336",
      "warning": "#ff9800",
      "info": "#2196F3"
    },
    "fonts": {
      "title": ["Arial", 22, "bold"],
      "heading": ["Arial", 12, "bold"],
      "normal": ["Arial", 10],
      "code": ["Consolas", 10],
      "status": ["Arial", 9]
    },
    "auto_save_config": true,
    "remember_window_position": true,
    "show_splash_screen": false
  },

  "export": {
    "html": {
      "template_style": "dark",
      "include_css": true,
      "include_statistics": true,
      "include_copyright": true,
      "auto_open_browser": false
    },
    "json": {
      "pretty_print": true,
      "include_metadata": true,
      "include_copyright": true,
      "compression": false
    },
    "txt": {
      "include_detailed_info": true,
      "include_statistics": true,
      "include_copyright": true,
      "line_separator": "unix"
    }
  },

  "logging": {
    "enabled": true,
    "level": "INFO",
    "file": "logs/card_sharing_analyzer.log",
    "max_size": "10MB",
    "backup_count": 5,
    "format": "%(asctime)s - %(levelname)s - %(message)s"
  },

  "notifications": {
    "expiry_warnings": {
      "enabled": true,
      "warn_days": [30, 7, 3, 1],
      "show_popup": true,
      "email_notifications": false
    },
    "server_status": {
      "notify_offline": true,
      "notify_online": false,
      "sound_alerts": false
    }
  },

  "performance": {
    "cache_results": true,
    "cache_duration": 300,
    "parallel_connections": true,
    "connection_pool_size": 10,
    "dns_cache": true,
    "dns_timeout": 5
  },

  "security": {
    "encrypt_passwords": false,
    "secure_connections": false,
    "verify_certificates": false,
    "connection_encryption": "none"
  },

  "defaults": {
    "config_file": "config/sample_servers.cfg",
    "export_directory": "exports/",
    "report_directory": "exports/reports/",
    "data_directory": "exports/data/",
    "temp_directory": "temp/"
  },

  "advanced": {
    "debug_mode": false,
    "verbose_logging": false,
    "show_raw_data": false,
    "enable_experimental_features": false,
    "custom_protocols": [],
    "plugin_directory": "plugins/",
    "auto_update_check": true
  },

  "metadata": {
    "created": "2025-01-20",
    "last_modified": "2025-01-20",
    "config_version": "1.0",
    "compatible_app_versions": ["3.0", "3.1"],
